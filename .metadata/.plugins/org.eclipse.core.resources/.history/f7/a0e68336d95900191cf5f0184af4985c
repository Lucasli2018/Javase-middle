package com.lucas.javase.collection.basic;


import java.util.ArrayList;
import java.util.List;

public class Main {

	public static void main(String[] args) {
		List<String> list = new ArrayList<>();
		list.add("Apple");
		list.add("Pear");
		list.add("Orange");
		list.add(1, "lucas");
		//1 foreach
		for (String s : list) {
			System.out.println(s);
		}
		//2 toString
		System.out.println(list);
	}

}
