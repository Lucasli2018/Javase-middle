	</loggers>
	<appenders>
		<console name="stdout">
			<patternLayout pattern="%d %p [%t]: %m%n"></patternLayout>
		</console>
		<RollingFile name="all" fileName="log/all.log">
		
		</RollingFile>
	</appenders>
</configuration>